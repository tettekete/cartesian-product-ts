{
  "name": "@tettekete/cartesian-product",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --runInBand",
    "build:clean": "rm -rf dist",
    "build:types": "tsc --project tsconfig.build.json",
    "build": "npm run build:clean && npm run build:types && node esbuild.config.mjs",
    "dev": "npm run build; node dist/index.js",
    "prepack": "npm run build",
    "lint": "eslint"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": {
        "import": "./dist/index.browser.js",
        "default": "./dist/index.browser.js"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "keywords": [],
  "author": "tettekete",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@stylistic/eslint-plugin": "^5.4.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.6.2",
    "cross-env": "^10.1.0",
    "esbuild": "0.25.10",
    "eslint": "^9.37.0",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0"
  },
  "files": [
    "dist"
  ],
  "type": "module"
}
